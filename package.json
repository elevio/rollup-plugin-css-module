{
  "name": "rollup-plugin-css-module",
  "description": "Rollup plugin for CSS modules",
  "version": "0.0.19",
  "license": "MIT",
  "main": "dist/rollup-plugin-css-module.cjs.js",
  "jsnext:main": "dist/rollup-plugin-css-module.es6.js",
  "files": [
    "dist",
    "README"
  ],
  "author": {
    "email": "paul@elev.io",
    "name": "Paul Veevers"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elevio/rollup-plugin-css-module.git"
  },
  "homepage": "https://github.com/elevio/rollup-plugin-css-module#readme",
  "bugs": {
    "url": "https://github.com/elevio/rollup-plugin-css-module/issues"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "css module",
    "postcss"
  ],
  "dependencies": {
    "colors": "^1.1.2",
    "css-modules-loader-core": "^1.0.0",
    "postcss": "^5.2.0",
    "postcss-remove-classes": "^1.0.2",
    "rollup": "^0.36.4",
    "rollup-pluginutils": "^1.2.0",
    "string-hash": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.9.0",
    "bootstrap": "^3.3.7",
    "cssnano": "^3.7.4",
    "eslint": "^3.11.1",
    "jsdom": "^9.2.1",
    "nyc": "^10.0.0",
    "postcss-nested": "^1.0.0",
    "rollup-plugin-babel": "^2.4.0",
    "standard": "^8.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 70,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "ava": {
    "files": [
      "./test/test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "test": "standard && nyc ava",
    "prebuild": "npm run test && rm -rf dist",
    "build": "rollup -c && cp src/css-module.js dist",
    "prepublish": "npm run build"
  }
}
